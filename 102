    <script src="/lib/lib.min.js?v=XuIVv2JrQbSFgR6fohQnG0v-0-HEcRCn-92q4TpAh-M"></script>

<script src="https://kit.fontawesome.com/f6434129d2.js" crossorigin="anonymous"></script>


    

    <script src="/lib/local.min.js?v=H1EsobNWpep2IKVrZHQpp8lGAxoRJIEeWdNvBubmnkc"></script>


    
     
    <script type="text/javascript">var relativeUrl = '';</script>
    

    <script src="/lib/pickmeup/pickmeup.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var currentLinkTemplate = "/njmvc/AppointmentWizard/11/102?date=_date_";
            var minDate = new Date("2021-04-04");
            var maxDate = new Date("2021-06-04"); // include the last date
            var selectedDate = new Date(2021, 5, 3);
            var availableDates = new Object();
            var initialDateKey = getDateKey(selectedDate);
            addAvailableDates(initialDateKey, ["2021-06-03T00:00:00"])

            pickmeup('#cal-picker',
                {
                    flat: true,
                    first_day: 0,
                    format: 'Y-m-d',
                    min: minDate,
                    max: maxDate,
                    render: function (date) {
                        var selectedMonthText = $(".pmu-month").text();
                        if (selectedMonthText.indexOf(",") > 0) {
                            var selectedMonthDate = new Date(selectedMonthText.replace(",", "1,"));
                            var dateKey = getDateKey(selectedMonthDate);
                            if (!(dateKey in availableDates)) {
                                availableDates[dateKey] = [];
                                loadDatesForMonth(selectedMonthDate);
                            }
                            if ($.inArray(date.getTime(), availableDates[dateKey]) < 0) {
                                return { disabled: true, class_name: 'pmu-disabled disabled pmu-button' };
                            }
                        }
                        return {};
                    }
                });

            pickmeup('#cal-picker').set_date(selectedDate);
            $("#cal-picker").on('pickmeup-change', function () {
                var selectedDate = pickmeup('#cal-picker').get_date(true);
                location.href = currentLinkTemplate.replace("_date_", selectedDate);
            });

            function addAvailableDates(key, dates) {
                availableDates[key] = [];
                $.each(dates, function (index, value) {
                    var timeToAdd = new Date(value.replace(/-/g, '/').replace('T', ' ')).getTime();
                    availableDates[key].push(timeToAdd);
                });
            }
            function getDateKey(date) {
                return (date.getMonth() + 1) + "/" + date.getFullYear();
            }

            function loadDatesForMonth(date) {
                var baseUrl = '/njmvc/CustomerCreateAppointments/GetAvailableDatesForMonth?duration=20&locationId=102&appointmentId=11';
                var url = baseUrl + "&date=" + date.toISOString();
                $("#loader").show();
                $.get(url)
                    .done(function (data) {
                        if (data != null) {
                            addAvailableDates(getDateKey(date), data);
                            pickmeup('#cal-picker').update();
                        }
                        $("#loader").hide();
                });


            }
        });
    </script>



    


